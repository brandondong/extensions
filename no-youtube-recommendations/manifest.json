{
  "manifest_version": 3,
  "name": "No Youtube Recommendations",
  "version": "0.1",
  "description": "Hides Youtube recommendations.",
  "content_scripts": [
    {
      "matches": [
        "https://*.youtube.com/*",
        "https://*.youtube-nocookie.com/*"
      ],
      "all_frames": true,
      "css": [
        "youtube.css"
      ]
    }
  ]
}